<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ThirdActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbarThird"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#ff7043"
        app:popupTheme="@style/AppTheme.PopupOverlay"
        app:title="Pihak Ketiga"
        app:theme="@style/ToolbarTheme"
        app:titleTextColor="@color/colorWhite" />

    <TextView
        android:id="@+id/t1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="17dp"
        android:textStyle="bold|italic"
        android:layout_marginTop="70dp"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="20dp"
        android:text="de.hdodenhof:circleimageview:2.1.0"/>
    <TextView
        android:id="@+id/t2"
        android:layout_below="@+id/t1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="16dp"
        android:layout_marginTop="10dp"
        android:textStyle="bold|italic"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="20dp"
        android:text="com.squareup.picasso:picasso:2.5.2"/>
    <TextView
        android:id="@+id/t3"
        android:layout_below="@+id/t2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="17dp"
        android:layout_marginTop="10dp"
        android:textStyle="bold|italic"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="20dp"
        android:text="com.squareup.okhttp:okhttp:2.6.0"/>
    <TextView
        android:id="@+id/t4"
        android:layout_below="@+id/t3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="17dp"
        android:textStyle="bold|italic"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="20dp"
        android:text="com.github.bumptech.glide:glide:4.0.0"/>
    <TextView
        android:id="@+id/t5"
        android:layout_below="@+id/t4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="17dp"
        android:textStyle="bold|italic"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="20dp"
        android:text="com.squareup.retrofit2:retrofit:2.4.0"/>
    <TextView
        android:id="@+id/t6"
        android:layout_below="@+id/t5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="17dp"
        android:textStyle="bold|italic"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="20dp"
        android:text="com.jakewharton:butterknife-compiler:8.8.1"/>

</RelativeLayout>